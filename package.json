{
	"name": "sol-analyze",
	"displayName": "sol-analyze",
	"description": "Static Analysis tool for Solidity",
	"version": "0.0.1",
	"configurations":[
		{
			"console": "integratedTerminal"
		}
	],
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sol-analyze.execute"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sol-analyze.execute",
				"title": "Sol-Analyze"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@solidity-parser/parser": "^0.14.0"
	}
}
